<section id="TrackerTitle">
    <div class="container">
        <div class="section-title">
            <div id="month">
                <div id="previousMonth"><i class="fa fa-arrow-left" style="visibility:visible;"></i></div>
                <h1 id="monthLabel"></h1>
                <div id="nextMonth"><i class="fa fa-arrow-right" style="visibility: visible;"></i></div>
            </div>
        </div>
    </div>
</section>

<div id="cal_sec">
    <div id="daysLabels"></div>
    <div id="calendar"></div>
    <div id="back_SC">
        <a asp-action="Index">Back to List</a>
    </div>
</div>

<div id="info">
    <ul id="taskLabels"></ul>
</div>

<div style="display: none;">
    <p id="lbl">@ViewBag.hLabel</p>
    <p id="id">@ViewBag.habitId</p>
    <p id="color">@ViewBag.hColor</p>
</div>
<!--Spinner-->
<div class="wrap">
    <div class="sticker"></div>
    <div class="msg">@ViewBag.hLabel</div>
</div>

   
<script src="~/assets/js/backup.js"></script>




